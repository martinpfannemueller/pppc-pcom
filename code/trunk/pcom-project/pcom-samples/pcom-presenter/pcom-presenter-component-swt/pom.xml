<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<parent>
		<artifactId>pcom-presenter</artifactId>
		<groupId>info.pppc.pcom</groupId>
		<version>1.0.1-SNAPSHOT</version>
	</parent>
	<groupId>info.pppc.pcom</groupId>
	<artifactId>pcom-presenter-component-swt</artifactId>
	<version>1.0.1-SNAPSHOT</version>
	<name>PCOM Presenter Component Swt</name>
	<description>A set of swt-based components for the PCOM presenter.</description>
	<build>
		<plugins>
			<plugin>
				<!-- Compile this for J2ME CDC. -->
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.3</source>
					<target>1.1</target>
					<bootclasspath>${java.cdc.path}</bootclasspath>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>info.pppc.base</groupId>
			<artifactId>base-core-system</artifactId>
		</dependency>
		<dependency>
			<groupId>info.pppc.base</groupId>
			<artifactId>base-extension-lease</artifactId>
		</dependency>
		<dependency>
			<groupId>info.pppc.base</groupId>
			<artifactId>base-ui-swt</artifactId>
		</dependency>
		<dependency>
			<groupId>info.pppc.pcom</groupId>
			<artifactId>pcom-core-container</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>info.pppc.pcom</groupId>
			<artifactId>pcom-ui-swt-container</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>info.pppc.pcom</groupId>
			<artifactId>pcom-presenter-component-common</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.github.mcpat.apistubs</groupId>
			<artifactId>cdc-1.1.2-stub</artifactId>
		</dependency>
		<dependency>
			<groupId>gnu.io</groupId>
			<artifactId>gnu-rxtx</artifactId>
		</dependency>
		<dependency>
			<groupId>swt</groupId>
			<artifactId>jface</artifactId>
		</dependency>
		<dependency>
			<groupId>org.eclipse.swt.win32.win32</groupId>
			<artifactId>x86</artifactId>
		</dependency>
		<dependency>
			<groupId>org.eclipse</groupId>
			<artifactId>swt</artifactId>
		</dependency>
		<dependency>
			<groupId>swt</groupId>
			<artifactId>runtime</artifactId>
		</dependency>
		<dependency>
			<groupId>org.eclipse</groupId>
			<artifactId>draw2d</artifactId>
		</dependency>
		<dependency>
			<groupId>com.jacob</groupId>
			<artifactId>jacob-project</artifactId>
		</dependency>
	</dependencies>
</project>